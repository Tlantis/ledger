page .dots {
  margin: 0 20rpx;
  height: 60rpx;
  width: 60rpx;
  border-radius: 50%;
  background-color: #556B2F;
}
page .dots image {
  /* transform: translate(0,-25%); */
  height: 100%;
  width: 100%;
}
page .outInButton {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
page .outInButton .outIn {
  padding: 20rpx;
  font-weight: 900;
}
page .outInButton .outInChoose {
  color: #ffaa00;
}
page .money {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  height: 70rpx;
  background-color: #CCC;
  font-weight: 900;
  color: #FFF;
}
page .money .moneyIcon {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
page .money .text {
  display: inline-block;
}
page .money .amountNum {
  margin-right: 20rpx;
  line-height: 70rpx;
}
page .icon {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  /* justify-content:space-evenly; */
}
page .icon .iconChild {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
