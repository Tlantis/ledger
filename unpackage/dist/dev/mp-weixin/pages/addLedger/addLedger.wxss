          flex-direction: column;
  margin: 20rpx 12rpx;
}
page .icon .iconChild .iconText {
  text-align: center;
}
page .iconHidden {
  display: none;
}
page .console {
  position: absolute;
  bottom: 0;
  background-color: #ebebeb;
}
