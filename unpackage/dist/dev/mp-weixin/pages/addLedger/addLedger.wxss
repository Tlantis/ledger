  border-left: #dddddd 1rpx solid;
  border-bottom: #dddddd 1rpx solid;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 50rpx;
}
page .console .consoleChild .addLedgerButton {
  border-left: #dddddd 1rpx solid;
  width: 375rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 50rpx;
}
page .console .otherButton {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
